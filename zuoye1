#include<stdio.h>
#include<math.h>
int main()
{
	long a,b,c;
	scanf("%d %d",&a,&b);
	c=a+b;
	int wei[100];
	int i,j,l;
	long k=c;
	if(c==0)
	printf("0");
	else
	{
	for(i=1;k!=0;i++)
	{
		wei[i]=abs(k%10);
		k=k/10;
	}
	if(c<0)
	printf("-");
	for(l=i-1;l>0;l--)
	{
		printf("%d",wei[l]);
		if(l!=1&&l%3==1)
		printf(",");
	}
}
	return 0;
 } 
